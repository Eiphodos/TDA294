\sorts {
       T;
}

\predicates{
	r(T); 	g(T);
	i(T,T);
}

\problem {
  (\forall T x; ((r(x) -> !g(x)) & (g(x) -> !r(x)))) &
  (\forall T x; \forall T y; (i(x,y) -> (r(x) | r(y))))
  ->
  (!\exists T x; \exists T y; (i(x,y) & g(x) & g(y))) &
  (\exists T x; \exists T y; i(x,y) -> \exists T z; r(z))
}
